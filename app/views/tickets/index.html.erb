<p id="notice"><%= notice %></p>

<h1>Tickets</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>Aticket</th>
      <th>Cticket</th>
      <th>Tfrom</th>
      <th>Tto</th>
      <th>Tclass</th>
      <th>Treturn</th>
      <th>Tvhour</th>
      <th>Tvdate</th>
      <th>Proof</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tickets.each do |ticket| %>
      <tr>
        <td><%= ticket.name %></td>
        <td><%= ticket.age %></td>
        <td><%= ticket.aticket %></td>
        <td><%= ticket.cticket %></td>
        <td><%= ticket.tfrom %></td>
        <td><%= ticket.tto %></td>
        <td><%= ticket.tclass %></td>
        <td><%= ticket.treturn %></td>
        <td><%= ticket.tvhour %></td>
        <td><%= ticket.tvdate %></td>
        <td><%= ticket.proof %></td>
        <td><%= ticket.user %></td>
        <td><%= link_to 'Show', ticket %></td>
        <td><%= link_to 'Edit', edit_ticket_path(ticket) %></td>
        <td><%= link_to 'Destroy', ticket, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ticket', new_ticket_path %>
