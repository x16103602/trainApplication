<%= simple_form_for(@ticket) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :age %>
    <%= f.input :aticket %>
    <%= f.input :cticket %>
    <%= f.input :tfrom %>
    <%= f.input :tto %>
    <%= f.input :tclass %>
    <%= f.input :treturn %>
    <%= f.input :tvhour %>
    <%= f.input :tvdate %>
    <%= f.input :proof %>
    <%= f.association :user %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
